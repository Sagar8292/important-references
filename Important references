Hide and Show

<div class="active_btn" style="text-align: center;">
<button id="btn1" class="active_phone">Bundled Pricing</button><br>
<button id="btn2">Regular Pricing</button><br></div>


<script>
jQuery(document).ready(function() {
  // Show div1 by default
  jQuery("#div1").show();
  jQuery("#div2").hide();
 
  jQuery("#btn1").click(function() {
	jQuery("#div1").show();
	jQuery("#div2").hide();
 	jQuery(".active_btn button").removeClass("active_phone");
	jQuery("#btn1").addClass("active_phone");
  });
 
  jQuery("#btn2").click(function() {
	jQuery("#div1").hide();
	jQuery("#div2").show();
 	jQuery(".active_btn button").removeClass("active_phone");
 	jQuery("#btn2").addClass("active_phone");
  });
});
</script>


