Making Site Live 

UPDATE wp_options SET option_value = replace(option_value, 'https://websitesupport24x7.com/reliable/', 'https://reliableautoservices.in/') WHERE option_name = 'home' OR option_name = 'siteurl';

UPDATE wp_posts SET guid = replace(guid, 'https://shopcmp.ca/GSCar/', 'https://gscarwashcentre.com/');

UPDATE wp_posts SET post_content = replace(post_content, 'https://shopcmp.ca/GSCar/', 'https://gscarwashcentre.com/');

UPDATE wp_postmeta SET meta_value = replace(meta_value,'https://shopcmp.ca/GSCar/', 'https://gscarwashcentre.com/');


